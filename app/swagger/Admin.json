{
    "openapi": "3.0.0",
    "info": {
        "title": "Kaynatt API",
        "description": "Mon API",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api"
        }
    ],
    "paths": {
        "/loginAdmin": {
            "post": {
                "tags": [
                    "Admins"
                ],
                "summary": "Connexion en tant qu'administrateur",
                "description": "Login as an admin and obtain a token.",
                "operationId": "7885f88d6ec6a08e83bfe4a1e112d633",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email_admin",
                                    "password"
                                ],
                                "properties": {
                                    "email_admin": {
                                        "type": "string",
                                        "example": "admin@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "password"
                                    },
                                    "role": {
                                        "type": "string",
                                        "example": "admin"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Connexion réussie",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status_message": {
                                            "type": "string",
                                            "example": "vous vous êtes connecté avec succès"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "JWT token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Email ou mot de passe incorrect"
                    },
                    "500": {
                        "description": "Erreur interne du serveur"
                    }
                }
            }
        },
        "/logoutAdmin": {
            "post": {
                "tags": [
                    "Admins"
                ],
                "summary": "Déconnexion de l'administrateur",
                "description": "Logout the admin.",
                "operationId": "016996c7bd362cda55c9d5e0be934981",
                "responses": {
                    "200": {
                        "description": "Déconnexion réussie",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Déconnexion réussie"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erreur interne du serveur"
                    }
                }
            }
        }
    }
}