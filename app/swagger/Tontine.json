{
    "openapi": "3.0.0",
    "info": {
        "title": "Kaynatt API",
        "description": "Mon API",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api"
        }
    ],
    "paths": {
        "/auth/ajouterTontine": {
            "post": {
                "tags": [
                    "Tontines"
                ],
                "summary": "Demande de creation d'une tontine",
                "description": "Demander la création d'une tontine.",
                "operationId": "881aef2034c3bc9723755b28fd59686f",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "libelle": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    },
                                    "montant": {
                                        "type": "number"
                                    },
                                    "regles": {
                                        "type": "string"
                                    },
                                    "date_de_debut": {
                                        "type": "string",
                                        "format": "date"
                                    },
                                    "periode": {
                                        "type": "string"
                                    },
                                    "etat": {
                                        "type": "string",
                                        "example": "en_attente"
                                    },
                                    "statutTontine": {
                                        "type": "string",
                                        "example": "en_attente"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Demande de création de tontine soumise avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status_code": {
                                            "type": "string",
                                            "example": "200"
                                        },
                                        "status_message": {
                                            "type": "string",
                                            "example": "Votre demande de création sera approuvée ou déclinée par l'administrateur de ce site. Vous recevrez une notification"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erreur interne du serveur"
                    }
                }
            }
        },
        "/ListeTontine": {
            "get": {
                "tags": [
                    "Tontines"
                ],
                "summary": " la liste de toutes les tontines",
                "description": "Obtenir la liste de toutes les tontines.",
                "operationId": "f6d46ca092b5a56d94cf71a9b4ba028e",
                "responses": {
                    "200": {
                        "description": "Liste de toutes les tontines récupérée avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status_code": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "status_message": {
                                            "type": "string",
                                            "example": "La liste de tous les tontines"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erreur interne du serveur"
                    }
                }
            }
        },
        "/createur_tontine/ListeTontineparCreateur/{user}": {
            "get": {
                "tags": [
                    "CreateurTontine"
                ],
                "summary": "Obtenir la liste de toutes les tontines créées par un créateur_tontine",
                "operationId": "0be9e6ee2714d13c905e104e7e5589c1",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ID de l'utilisateur créateur des tontines",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Liste de toutes les tontines créées par l'utilisateur récupérée avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status_code": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "status_message": {
                                            "type": "string",
                                            "example": "La liste de toutes les tontines créées par l'utilisateur"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Utilisateur non trouvé"
                    },
                    "500": {
                        "description": "Erreur interne du serveur"
                    }
                }
            }
        },
        "admin/AcceptedTontine/{tontines}": {
            "post": {
                "tags": [
                    "Admins"
                ],
                "summary": "Accepter la création d'une tontine et notifier le createur",
                "description": "Accepter la création d'une tontine.",
                "operationId": "05f2b0480297fb36921cb7f19b5354e7",
                "parameters": [
                    {
                        "name": "tontines",
                        "in": "path",
                        "description": "ID de la tontine à accepter",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "La demande de création de tontine a été acceptée",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "La demande de création de tontine a été acceptée"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "La tontine a déjà été acceptée",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "La tontine a déjà été acceptée"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Tontine non trouvée"
                    },
                    "500": {
                        "description": "Erreur interne du serveur"
                    }
                }
            }
        },
        "admin/RefuseTontine/{tontine}": {
            "post": {
                "tags": [
                    "Admins"
                ],
                "summary": "Refuser la création d'une tontine et notifier le createur ",
                "description": "Refuser la création d'une tontine.",
                "operationId": "c0588cac9573138858b0734d55a693b2",
                "parameters": [
                    {
                        "name": "tontine",
                        "in": "path",
                        "description": "ID de la tontine à refuser",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "La demande de création de tontine a été refusée",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "La demande de création de tontine a été refusée"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "La tontine a déjà été refusée",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "La tontine a déjà été refusée"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Tontine non trouvée"
                    },
                    "500": {
                        "description": "Erreur interne du serveur"
                    }
                }
            }
        },
        "admin/ListeTontineAccepte": {
            "get": {
                "tags": [
