{
    "openapi": "3.0.0",
    "info": {
        "title": "Kaynatt API",
        "description": "Mon API",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api"
        }
    ],
    "paths": {
        "/auth/fairePaiement/{gestionCycle}": {
            "post": {
                "tags": [
                    "Paiement Participant"
                ],
                "summary": "Effectuer un paiement pour un cycle de gestion donné",
                "description": "Effectue un paiement pour un cycle de gestion donné dans le système.",
                "operationId": "effectuerPaiement",
                "parameters": [
                    {
                        "name": "gestionCycle",
                        "in": "path",
                        "description": "ID du cycle de gestion",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Données requises pour effectuer le paiement",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "date_paiement": {
                                        "description": "Date du paiement",
                                        "type": "string",
                                        "format": "date"
                                    },
                                    "montant_paiement": {
                                        "description": "Montant du paiement",
                                        "type": "number",
                                        "format": "float"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Succès - Le paiement a été effectué avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status_code": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "status_message": {
                                            "type": "string",
                                            "example": "Le paiement a été effectué avec succès"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Requête incorrecte - Veuillez vérifier les paramètres de la requête"
                    },
                    "401": {
                        "description": "Non autorisé - Veuillez vous connecter pour effectuer cette action"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}